loadstring(game:HttpGet"https://pastebin.com/raw/AzgUz0Pg")()
-- ANTI AFK BLOCK --
loadstring(game:HttpGetAsync('https://raw.githubusercontent.com/igaslightwomen/aid/main/anti%20afk', true))()

local UI = Material.Load({
     Title = "Mew Menu",
     Style = 3,
     SizeX = 250,
     SizeY = 330,
     Theme = "Mocha"
})

local Page = UI.New({
    Title = "Main"
})

Page.Dropdown({
    Text = "UI Theme",
    Callback = function(value)
    
    if(value == "Light") then loadstring(game:HttpGet"https://pastebin.com/raw/yQGz6Exc")()
    elseif(value == "Dark") then loadstring(game:HttpGet"https://pastebin.com/raw/MkQuRAm5")()
    elseif(value == "Mocha") then loadstring(game:HttpGet"https://pastebin.com/raw/5vdTdHbf")()
    elseif(value == "Aqua") then loadstring(game:HttpGet"https://pastebin.com/raw/fTbtp5e4")()
    elseif(value == "Jester") then loadstring(game:HttpGet"https://pastebin.com/raw/sExn9x7Y")()    
    end
    end,
    Options = {"Light", "Dark", "Mocha", "Aqua", "Jester"}
})

Page.Button({
    Text = "Pokedollar Giver",
    Callback = function()
--infinite pokedollars script
loadstring(game:HttpGet"https://pastebin.com/raw/HCDhirg7")()   
    end
})

